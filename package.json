{
    "name": "production-project",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start-old": "webpack-dev-server --mode development --open",
        "build:dev": "webpack --config ./webpack/dev.config.js",
        "build:prod": "webpack --config ./webpack/prod.config.js",
        "build": "webpack --config ./webpack/prod.config.js",
        "start": "NODE_ENV=production node ./server.js",
        "dev": "NODE_ENV=development node ./server.js",
        "dev-webpack": "webpack-dev-server --mode development --open",
        "dev-server": "nodemon server.js",
        "dev-start": "npm run dev-webpack & npm run dev-server",
        "dev:hot": "NODE_ENV=development nodemon ./server.js & webpack-dev-server --config ./webpack/dev.config.js --open --hot --inline --progress --colors --watch --content-base ./",
        "test": "echo \"-- add tests here NODE_ENV=test jest\"",
        "test-coverage": "echo \"-- add tests here NODE_ENV=test jest --coverage\"",
        "test-watch": "NODE_ENV=test jest --watch",
        "test-updateSnapshot": "NODE_ENV=development jest --updateSnapshot",
        "add-dep": "bash ./scripts/npm-docker/add-dep.sh",
        "update-dep": "bash ./scripts/npm-docker/update-dep.sh",
        "image-remove": "docker image rm dbsites/namethatcard-dependencies --force",
        "image-build": "docker build -t dbsites/namethatcard-dependencies -f Dockerfile-dev .",
        "docker-remove-containers": "docker rm $(docker ps -a -q -f 'name=namethatcard') --force",
        "docker-remove-images": "docker rmi $(docker images dbsites/namethatcard* -q) --force",
        "docker-remove-volumes": "docker volume rm $(docker volume ls -q -f name=namethatcard) --force",
        "docker-build:dev": "docker-compose run --rm --service-ports build-dev",
        "docker-dev": "docker-compose -f docker-comp-dev.yml up",
        "docker-test": "docker-compose -f docker-comp-test.yml up --abort-on-container-exit",
        "docker-dev:hot": "docker-compose -f docker-comp-dev.yml -f docker-comp-dev-hot.yml up",
        "docker-stop": "docker stop $(docker ps -a -q --filter status=running)",
        "docker-remove-all:danger": "docker rm $(docker ps -a -q -f 'name=namethatcard') --force && docker rmi $(docker images dbsites/namethatcard* -q) --force && docker volume rm $(docker volume ls -q -f name=name-that-card) --force && exit"
    },
